{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "audio-replacer",
  "version": "5.0.0",
  "identifier": "com.audio-replacer.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Audio Replacer 5",
        "width": 1200,
        "minWidth": 1200,
        "height": 750,
        "minHeight": 750,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
