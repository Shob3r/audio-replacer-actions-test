{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    "http:default",
    "fs:default",
    "shell:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "log:default",
    "process:default",
    "upload:default",
    "store:default"
  ]
}